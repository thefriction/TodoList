<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   
    <link rel="stylesheet" href="styles.css">
    <title>todoList</title>
</head>
<body>
    <div class="box" id="heading">
    <h1><%= kindOfDay %></h1>
    </div>
    <div class="box">
    <ul>
    <%items.forEach(function(item)  { %>
        <div class="item">
            <input type="checkbox">
        <p><%=item.listBody%></p>
    </div>
    <%})%>
    </ul>

    
    <form class="item" method="POST" action="/">
        <input type="text" class="" name = "todoItem" placeholder="NEW ITEM" autocomplete="off">
        <button type="submit">+</button>
    </form>
</div>
</body>
</html>